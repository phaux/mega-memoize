{
  "name": "@mega/memoize",
  "version": "0.2.0",
  "license": "MIT",
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noUncheckedIndexedAccess": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/async": "jsr:@std/async@^1.0.10"
  },
  "exports": {
    ".": "./memoize.ts",
    "./async": "./memoizeAsync.ts",
    "./cache/memory": "./memoryCache.ts",
    "./cache/domStorage": "./domStorageCache.ts",
    "./cache/denoKv": "./denoKvCache.ts",
    "./keyNormalizer": "./KeyNormalizer.ts",
    "./valueSerializer": "./ValueSerializer.ts"
  },
  "publish": {
    "exclude": [".*"]
  },
  "lint": {
    "rules": {
      "include": ["no-console"]
    }
  },
  "lock": false
}
